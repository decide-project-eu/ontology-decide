        <rdfs:range rdf:resource="http://www.purl.org/decide#EventType"/>
        <rdfs:comment>Associates an Event with its EventType, describing the nature or category of the event 
    (e.g., Sampling, Treatment, Inspection, Vaccination).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/decide#hasMeasurement -->

