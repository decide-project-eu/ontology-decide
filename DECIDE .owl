        <rdfs:range rdf:resource="http://www.purl.org/decide#MeasurementType"/>
        <rdfs:comment>Associates a Measurement with a MeasurementType that describes what kind of value is being measured 
    (e.g., body weight, temperature, pH level,)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/decide#hasResult -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/decide#hasResult">
