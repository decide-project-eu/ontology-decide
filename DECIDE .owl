    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/decide#hasUnit -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/decide#hasUnit">
        <rdfs:domain rdf:resource="http://www.purl.org/decide#Measurement"/>
