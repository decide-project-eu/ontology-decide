    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/decide#identifies -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/decide#identifies">
        <rdfs:domain rdf:resource="http://www.purl.org/decide#Event"/>
        <rdfs:domain rdf:resource="http://www.purl.org/decide#Sample"/>
        <rdfs:domain rdf:resource="http://www.purl.org/decide#SampleResult"/>
        <rdfs:range rdf:resource="http://www.purl.org/decide#Pathogen"/>
        <rdfs:comment>Links a SampleResult to the Pathogen that was identified in the sample. 
    This is used to record confirmed detections of pathogens through diagnostic testing.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/decide#involves -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/decide#involves">
        <rdfs:domain rdf:resource="http://www.purl.org/decide#Event"/>
        <rdfs:range rdf:resource="http://www.purl.org/decide#SpeciesGroup"/>
        <rdfs:comment>Relates an Event to the SpeciesGroup that was involved in or affected by the event.
    This helps to contextualize the event with respect to the biological group it targeted.</rdfs:comment>
    </owl:ObjectProperty>
    


