        <rdfs:range rdf:resource="http://www.purl.org/decide#SampleType"/>
        <rdfs:comment>Associates a Sample with a SampleType indicating the kind of material collected, such as blood, tissue, feces, or water.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.purl.org/decide#hasStatistics -->

    <owl:ObjectProperty rdf:about="http://www.purl.org/decide#hasStatistics">
        <rdfs:domain rdf:resource="http://www.purl.org/decide#Event"/>
        <rdfs:range rdf:resource="http://www.purl.org/decide#Statistics"/>
        <rdfs:comment>Links an Event to one or more Statistic instances that summarize outcomes or observations related to that event. 
